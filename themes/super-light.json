  "statusBar.debuggingBackground": "#FF8D00",
  "statusBar.debuggingForeground": "#FF6C6C",
  "statusBar.noFolderBackground": "#FFFFFF",
  "statusBarItem.hoverBackground": "#FFFFFF",
  "statusBarItem.remoteBackground": "#B529FE",
  "statusBarItem.remoteForeground": "#FFFFFF",
// the tabs (above editor)
  "tab.activeBackground": "#a80ffa",
  "tab.activeBorder": "#B529FE",
  "tab.activeForeground": "#b91212",
  "tab.activeModifiedBorder": "#B529FE",
  "tab.border": "#FFFFFF",
  "tab.inactiveBackground": "#FFFFFF",
  "tab.inactiveForeground": "#b91212",
  "tab.unfocusedActiveBackground": "#B529FE40",
  "tab.unfocusedActiveBorder": "#B529FE",
  "tab.unfocusedActiveForeground": "#860b04",
  "tab.unfocusedInactiveBackground": "#B529FE10",
  "tab.unfocusedInactiveForeground": "#860b04",
// the terminal colors
  "terminal.ansiBlack": "#FFFFFF",
  "terminal.ansiBlue": "#E93BFF",
  "terminal.ansiBrightBlack": "#B529FE",
  "terminal.ansiBrightBlue": "#E93BFF",
  "terminal.ansiBrightCyan": "#FFD25A",
  "terminal.ansiBrightGreen": "#990404",
  "terminal.ansiBrightMagenta": "#FF8D00",
  "terminal.ansiBrightRed": "#FFD827",
  "terminal.ansiBrightWhite": "#FF6C6C",
  "terminal.ansiBrightYellow": "#FF8D00",
  "terminal.ansiCyan": "#FFD25A",
  "terminal.ansiGreen": "#990404",
  "terminal.ansiMagenta": "#FF8D00",
  "terminal.ansiRed": "#FFD827",
  "terminal.ansiWhite": "#FF6C6C",
  "terminal.ansiYellow": "#FF8D00",
