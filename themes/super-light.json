  "panelTitle.activeBorder": "#B529FE",
  "panelTitle.activeForeground": "#FF6C6C",
  "panelTitle.inactiveForeground": "#FF1A0D",

  "peekView.border": "#FFFFFF30",
  "peekViewEditor.background": "#FF1A0D05",
  "peekViewEditor.matchHighlightBackground": "#B529FE7f",
