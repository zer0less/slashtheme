  "tree.indentGuidesStroke": "#0f0f0f",
  "widget.shadow": "#FFFFFF",
  },
  "tokenColors": [
  {
    "name": "Comments",
    "scope": [
      "comment"
    ],
    "settings": {
      "foreground": "#FF8D00",
      "fontStyle": ""
    }
  },
  {
    "name": "Variables Other",
    "scope": [
    "variable.other.readwrite.js",
    "variable.other.object.js",
    "variable.parameter.js"
    ],
    "settings": {
    "foreground": "#FF1A0D"
    }
  },
  {
    "name": "Keywords",
    "scope": [
      "keyword",
      "storage.type.js",
      "storage.type.function.python",
      "constant.language",
      "keyword.control",
      "keyword.control.conditional.js"
    ],
    "settings": {
      "foreground": "#FF8D00",
      "fontStyle": ""
    }
  },
  {
    "name": "Functions/Methods",
    "scope": [
      "entity.name.function",
      "support.function",
      "support.class",
      "entity.name",
      "support.type.python"
    ],
    "settings": {
      "foreground": "#FF1A0D",
      "fontStyle": "bold"
    }
  },
  {
    "name": "Strings",
    "scope": [
      "string"
    ],
    "settings": {
      "foreground": "#f06509",
      "fontStyle": ""
    }
  },
  {
    "name": "Variable",
    "scope": [
      "variable",
    ],
    "settings": {
      "foreground": "#FF8D00",
      "fontStyle": ""
    }
  },
  {
    "name": "Numbers",
    "scope": [
      "constant.numeric"
    ],
    "settings": {
      "foreground": "#f06509",
      "fontStyle": ""
    }
  },
  {
    "name": "Punctuation & Operators",
    "scope": [
      "keyword.operator.assignment.js",
      "keyword.operator.assignment.python",
      "punctuation",
      "punctuation.definition.comment",
      "meta.brace.square.js",
      "meta.brace.round.js",
      "punctuation.definition",
      "punctuation.definition.string.begin.js",
      "punctuation.definition.string.end.js",
      "punctuation.definition.string.begin.python",
      "punctuation.definition.string.end.python",
      "punctuation.separator.comma.js",
      "keyword.operator.relational.js"
    ],
    "settings": {
      "foreground": "#860b04",
      "fontStyle": ""
    }
  },
  {
    "scope": [
    "markup.inserted"
    ],
    "settings": {
    "foreground": "#FFD827"
    }
  },
  {
    "scope": [
    "markup.deleted"
    ],
    "settings": {
    "foreground": "#FF8D00"
    }
  },
  {
    "scope": [
    "markup.changed"
    ],
    "settings": {
    "foreground": "#990404"
    }
  },
  {
    "scope": [
    "string.regexp"
    ],
    "settings": {
    "foreground": "#FFD25A"
    }
  },
  {
    "scope": [
    "constant.character.escape"
    ],
    "settings": {
    "foreground": "#FFD25A"
    }
  },
  {
    "scope": [
    "*url*",
    "*link*",
    "*uri*"
    ],
    "settings": {
    "fontStyle": "underline"
    }
  },
  {
    "scope": [
    "source.js constant.other.object.key.js string.unquoted.label.js"
    ],
    "settings": {
    "foreground": "#FF1A0D",
    "fontStyle": "italic"
    }
  },
  {
    "scope": [
    "support.type.property-name.json"
    ],
    "settings": {
    "foreground": "#FF1A0D"
    }
  },
  {
    "scope": [
    "text.html.markdown",
    "punctuation.definition.list_item.markdown"
    ],
    "settings": {
    "foreground": "#FF1A0D"
    }
  },
  {
    "scope": [
    "text.html.markdown markup.inline.raw.markdown"
    ],
    "settings": {
    "foreground": "#FF8D00"
    }
  },
  {
    "scope": [
    "text.html.markdown markup.inline.raw.markdown",
    "punctuation.definition.raw.markdown"
    ],
    "settings": {
    "foreground": "#a80ffa"
    }
  },
  {
    "scope": [
    "text.html.markdown meta.dummy.line-break"
    ],
    "settings": {}
  },
  {
    "scope": [
    "markdown.heading",
    "markup.heading | markup.heading entity.name",
    "markup.heading.markdown punctuation.definition.heading.markdown",
    "entity.name.section.markdown"
    ],
    "settings": {
    "foreground": "#FF8D00",
    "fontStyle": "bold"
    }
  },
  {
    "scope": [
    "markup.underline"
    ],
    "settings": {
    "foreground": "#E93BFF",
    "fontStyle": "underline"
    }
  },
  {
    "scope": [
    "markup.strike"
    ],
    "settings": {
    "fontStyle": ""
    }
  },
  {
    "scope": [
    "markup.quote punctuation.definition.blockquote.markdown"
    ],
    "settings": {
    "foreground": "#a80ffa"
    }
  },
  {
    "scope": [
    "markup.quote"
    ],
    "settings": {
    "fontStyle": "italic"
    }
  },
  {
    "scope": [
    "string.other.link.description.title.markdown"
    ],
    "settings": {
    "foreground": "#FF8D00"
    }
  },
  {
    "scope": [
    "constant.other.reference.link.markdown"
    ],
    "settings": {
    "foreground": "#FF8D00"
    }
  },
  {
    "scope": [
    "markup.raw.block"
    ],
    "settings": {
    "foreground": "#FF8D00"
    }
  },
  {
    "scope": [
    "punctuation.definition.raw.markdown",
    "punctuation.definition.markdown"
    ],
    "settings": {
    "foreground": "#FF8D00"
    }
  },
  {
    "scope": [
    "variable.language.fenced.markdown"
    ],
    "settings": {
    "foreground": "#B529FE"
    }
  },
  {
    "scope": [
    "meta.separator"
    ],
    "settings": {
    "foreground": "#A7A8AF",
    "fontStyle": "bold"
    }
  },
  {
    "scope": "token.info-token",
    "settings": {
    "foreground": "#B529FE"
    }
  },
  {
    "scope": "token.warn-token",
    "settings": {
    "foreground": "#FF8D00"
    }
  },
  {
    "scope": "token.error-token",
    "settings": {
    "foreground": "#FF1A0D"
    }
  },
  {
    "scope": "token.debug-token",
    "settings": {
    "foreground": "#FF8D00"
    }
  }
  ]
}